{
	"schemaVersion": 1,
	"id": "nutritionbts",
	"version": "${version}",
	"name": "NutritionBTS",
	"description": "This mod is about nutritions",
	"authors": [
		"Aros"
	],
	"contact": {
		"homepage": "https://github.com/BehindTheScenery",
		"sources": "https://github.com/BehindTheScenery/NutritionBTS"
	},
